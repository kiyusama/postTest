<h1>Posts#new</h1>
<h2>テスト<h2>
<%= form_with model:@post, url:"/posts", local:true do |f| %>
<h3>タイトル</h3>
<%= f.text_field :title %>
<h3>本文</h3>
<%= f.text_area :body %>
<h3>ノート</h3>
<%= f.text_field :note %>
<h3>ボタン</h3>
<%= f.submit "投稿" %>
<% end %>
<p>Find me in app/views/posts/new.html.erb</p>
